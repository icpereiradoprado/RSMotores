
@{
    ViewBag.TItle = "CAdastro de Usuário";
    Layout = "_Layout";
}

<form asp-controller="Usuario" asp-action="Create" method="post">
    <div style="margin-bottom: 10px;"><input type="text" id="nickName" name="nickName" placeholder="Nome"/></div>
    <div style="margin-bottom: 10px;"><input type="text" id="cpf" name="cpf" placeholder="CPF"/></div>
    <div style="margin-bottom: 10px;"><input type="email" id="email" name="email" placeholder="Email"/></div>
    <div style="margin-bottom: 20px;"><input type="password" id="password" name="password" placeholder="Senha"/></div>
    <div><input type="submit" value="Cadastrar"/></div>
</form>
